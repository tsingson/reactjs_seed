{
  "name": "angular2-webpack-seed",
  "version": "0.0.0",
  "description": "An Angular2 WebPack seed featuring hot-module-reloading of components",
  "main": "",
  "scripts": {
    "prestart": "npm install",
    "build": "./node_modules/.bin/webpack -d",
    "watch": "./node_modules/.bin/webpack -d --watch",
    "server": "./node_modules/.bin/webpack-dev-server -d --inline --colors --display-error-details --content-base public",
    "start": "npm install && npm run build && npm run server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },

  "author": "tsingson <tsingson@me.com>",
  "license": "MIT",

  "dependencies": {
    "angular2": "2.0.0-alpha.18",
    "rtts_assert": "2.0.0-alpha.18",
    "zone.js": "^0.4.1"
  },
  "devDependencies": {
    "babel-core": "^4.7.3",
    "babel-loader": "^4.1.0",
    "css-loader": "^0.9.1",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.6.0",
    "file-loader": "^0.8.1",
    "imports-loader": "^0.6.3",
    "jade": "^1.9.2",
    "jade-loader": "^0.7.1",
    "json-loader": "^0.5.1",
    "style-loader": "^0.8.3",
    "traceur": "0.0.86",
    "traceur-compiler-loader": "^1.0.6",
    "typescript-loader": "^1.1.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.2",
    "webpack-dev-server": "^1.7.0"
  }
}
